<html>

<head>
  <!-- STYLES -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap-theme.min.css">
  <link href="css/loading.css" rel="stylesheet" type="text/css" />

  <!-- META INFORMATION -->
  <title>Loading...</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
  <script src="//cdn.jsdelivr.net/blocks/0.3.1/blocks.min.js"></script>
</head>

<body>
  <div id="background">&nbsp;</div>
  <div class="container center">
    <div class="row">
      <div class="col-sm-12">
        <h1 class="title">Welcome to <b>Casual Bananas</b></h1>
      </div>
    </div>

    <div class="row">
      <div class="col col-sm-4">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Server info</h3>
          </div>
          <div class="panel-body">
            <p style="line-height:38px;">
              <img class="icon" src="img/icon_players.png"><div id="playercount">0/0 players</div>
              <br />
              <img class="icon" src="img/icon_map.png"><div id="map">undefined</div>
              <br />
              <img class="icon" src="img/icon_ip.png">127.0.0.1:27015
              <br />
            </p>
          </div>
        </div>

        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">About me</h3>
          </div>
          <div class="panel-body">
            <p style="line-height:38px;">
              <img class="icon" src="img/icon_user.png" /><div id="playername">Undefined</div>
              <br />
              <img class="icon" src="img/icon_id.png"><div id="steamid">STEAM_0:0:0</div>
              <br />
              <img class="icon" src="img/icon_bananas.png" /><div id="bananas">0 bananas</div>
              <br />
              <img class="icon" src="img/icon_time.png" /><div id="time">0 hours</div>
            </p>
          </div>
        </div>
      </div>

      <div class="col col-sm-8">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Chat</h3>
          </div>
          <div class="panel-body">
            CHATBOX GOES HERE
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col col-sm-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Joining</h3>
          </div>
          <div class="panel-body">
            <div class="progress">
              <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                <span class="sr-only">100% Complete</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

  </div>
  <!-- SCRIPTS -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
  <script src="//cdn.socket.io/socket.io-1.3.5.js"></script>
  <script src="/js/loading.js"></script>
</body>

</html>
